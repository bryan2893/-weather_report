{
    "name": "Wheather Report",
    "manifest_version": 2,
    "description": "",
    "version": "1.0",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Extension Finder"
    },
    "background":{
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["cookies","tabs"]
}